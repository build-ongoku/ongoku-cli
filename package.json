{
  "name": "@ongoku/ongoku-cli",
  "version": "0.1.0",
  "description": "CLI tool for interacting with Ongoku projects",
  "keywords": [
    "ongoku",
    "cli",
    "development",
    "workflow"
  ],
  "homepage": "https://github.com/build-ongoku/ongoku-cli#readme",
  "bugs": {
    "url": "https://github.com/build-ongoku/ongoku-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/build-ongoku/ongoku-cli.git"
  },
  "license": "MIT",
  "author": "Ongoku Team",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ongoku": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "publish": "npm run build && npm version patch && git add package.json && git commit -m 'chore: bump version' && git push && npm publish"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^10.0.1",
    "conf": "^10.2.0",
    "date-fns": "^2.30.0",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "open": "^8.4.2",
    "ora": "^5.4.1",
    "simple-git": "^3.19.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
